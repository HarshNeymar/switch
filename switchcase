#include <stdio.h> 
void main() 
{
int choice;

printf("Enter your choice: ");
scanf("%d,&choice);

switch(choice)
{
case 1:
{
printf("Pizza,Rs 239");
break:
}

case 2:
{
printf("Burger,Rs 129");
break;
}

case 3:
{
printf("Pasta,Rs 179");
break;
}

case 4:
{
printf("French Fries");
break;
}

case 5:
{
printf("Sandwich,Rs 149");
break;
}

}

}
